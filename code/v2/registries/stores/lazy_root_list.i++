      /**
       * push_front( )
       * 
       * complexity is (probably) O(1)
       * @author KHJ
       *
       */
      template<typename N>
      void lazy_root_list<N>::inject( N* S ) 
      { 
        root_list_.push_back( S );
        //++size_;
        //size_ += pow( 2, (*S).height( ) + 1) - 1;
      }

      /**
       * pop_front( )
       * 
       * complexity is (probably) O(1)
       * @author KHJ
       *
       */
      template<typename N>
      N* lazy_root_list<N>::eject( ) 
      { 
        N* S = root_list_.back( ); 
        root_list_.pop_back( ); 
        //--size_;// -= pow( 2, (*S).height( ) + 1) - 1;
        return S;
      }

               
